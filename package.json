    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "electron-is-dev": "^2.0.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "framer-motion": "^12.23.12",
