{
  "name": "my-electron-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix frontend\" \"electron .\"",
    "build": "npm run build --prefix frontend",
    "start": "npm run build --prefix frontend && electron .",
    "dist": "npm run build --prefix frontend && electron-builder"
  },
  "build": {
    "appId": "com.myapp.desktop",
    "productName": "MyApp",
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "@monaco-editor/react": "^4.7.0",
    "@react-oauth/google": "^0.12.2",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.11",
    "tectonic": "^2.1.2",
    "tmp": "^0.2.5",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "electron": "^31.0.0",
    "electron-builder": "^24.0.0"
  }
}
